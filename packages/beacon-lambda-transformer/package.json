{
  "name": "@airgap/beacon-lambda-transformer",
  "version": "3.2.0",
  "description": "A package that transforms Tezos internal operations into a Michelson lambda to be executed inside a smart contract.",
  "author": {
    "name": "Rodrigo Quelhas",
    "email": "r.quelhas@papers.ch"
  },
  "homepage": "https://walletbeacon.io",
  "license": "ISC",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "directories": {
    "lib": "dist/esm",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airgap-it/beacon-sdk.git"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "test": "TS_NODE_PROJECT='tsconfig-cjs.json' nyc mocha --require ts-node/register --require source-map-support/register --full-trace --timeout 40000 ./__tests__/**/**.spec.ts"
  },
  "bugs": {
    "url": "https://github.com/airgap-it/beacon-sdk/issues"
  },
  "dependencies": {
    "axios": "0.27.2"
  }
}
